{
    "name": "advancedhosters/hipanel",
    "type": "yii2-extension",
    "description": "AdvancedHosters implementation of HiPanel",
    "keywords": [
        "advanced hosters",
        "hipanel",
        "control panel",
        "hosting",
        "domains"
    ],
    "homepage": "https://advancedhosters.com/",
    "license": "proprietary",
    "support": {
        "email": "support@advancedhosters.com",
        "source": "https://github.com/advancedhosters/hipanel",
        "issues": "https://github.com/advancedhosters/hipanel/issues",
        "wiki": "https://github.com/advancedhosters/hipanel/wiki"
    },
    "authors": [
        {
            "name": "Andrii Vasyliev",
            "email": "sol@hiqdev.com",
            "homepage": "https://hipanel.com/"
        },
        {
            "name": "Dmitry Naumenko",
            "email": "d.naumenko.a@gmail.com",
            "homepage": "http://silverfire.me/"
        }
    ],
    "require": {
        "hiqdev/hipanel-core": "*@dev",
        "hiqdev/hipanel-module-client": "*@dev",
        "hiqdev/hipanel-client-debt": "<2.0 || dev-master",
        "hiqdev/hipanel-module-ticket": "*@dev",
        "hiqdev/hipanel-module-server": "*@dev",
        "hiqdev/hipanel-module-finance": "*@dev",
        "hiqdev/hipanel-module-hosting": "*@dev",
        "hiqdev/hipanel-module-dns": "*@dev",
        "hiqdev/hipanel-module-document": "*@dev",
        "hiqdev/hipanel-module-stock": "*@dev",
        "hiqdev/hipanel-module-mailing": "*@dev",
        "hiqdev/hisite-module-news": "*@dev",
        "hiqdev/hipanel-module-dashboard": "*@dev",
        "hiqdev/yii2-theme-adminlte": "<2.0 || dev-master"
    },
    "require-dev": {
        "hiqdev/hidev-php": "<2.0",
        "advancedhosters/hidev-advancedhosters": "<2.0",
        "codeception/codeception": "*"
    },
    "autoload": {
        "psr-4": {
            "advancedhosters\\hipanel\\": "src",
            "advancedhosters\\hipanel\\tests": "tests"
        }
    },
    "extra": {
        "config-plugin": {
            "web": "src/config/web.php"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
