{
    "name": "advancedhosters/hipanel",
    "type": "yii2-extension",
    "description": "AdvancedHosters implementation of HiPanel",
    "keywords": [
        "advanced hosters",
        "hipanel",
        "control panel",
        "hosting",
        "domains"
    ],
    "homepage": "https://advancedhosters.com/",
    "license": "proprietary",
    "support": {
        "email": "support@advancedhosters.com",
        "source": "https://github.com/advancedhosters/hipanel",
        "issues": "https://github.com/advancedhosters/hipanel/issues",
        "wiki": "https://github.com/advancedhosters/hipanel/wiki"
    },
    "authors": [
        {
            "name": "Andrii Vasyliev",
            "email": "sol@hiqdev.com",
            "homepage": "https://hipanel.com/"
        },
        {
            "name": "Dmitry Naumenko",
            "email": "d.naumenko.a@gmail.com",
            "homepage": "http://silverfire.me/"
        }
    ],
    "require": {
        "hiqdev/hipanel-core": "dev-php8",
        "hiqdev/hipanel-module-client": "dev-php8",
        "hiqdev/hipanel-module-ticket": "dev-php8",
        "hiqdev/hipanel-module-server": "dev-php8",
        "hiqdev/hipanel-server-order": "*@dev",
        "hiqdev/hipanel-module-finance": "dev-php8",
        "hiqdev/hipanel-module-hosting": "dev-php8",
        "hiqdev/hipanel-module-ipam": "dev-php8",
        "hiqdev/hipanel-module-dns": "dev-php8",
        "hiqdev/hipanel-module-document": "dev-php8",
        "hiqdev/hipanel-module-stock": "dev-php8",
        "hiqdev/hipanel-module-mailing": "dev-php8",
        "hiqdev/hisite-module-news": "*@dev",
        "hiqdev/hipanel-module-dashboard": "dev-php8",
        "hiqdev/hipanel-module-integrations": "dev-php8",
        "hiqdev/hipanel-client-debt": "dev-php8",
        "hiqdev/yii2-theme-adminlte": "dev-php8"
    },
    "require-dev": {
        "hiqdev/hidev-php": "<2.0",
        "advancedhosters/hidev-advancedhosters": "<2.0",
        "codeception/codeception": "*"
    },
    "autoload": {
        "psr-4": {
            "advancedhosters\\hipanel\\": "src",
            "advancedhosters\\hipanel\\tests\\": "tests"
        }
    },
    "extra": {
        "config-plugin": {
            "web": "config/web.php"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
