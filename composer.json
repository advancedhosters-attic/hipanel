{
    "name": "advancedhosters/hipanel",
    "type": "yii2-extension",
    "description": "AdvancedHosters implementation of HiPanel",
    "keywords": [
        "advanced hosters",
        "hipanel",
        "control panel",
        "hosting",
        "domains"
    ],
    "homepage": "https://advancedhosters.com/",
    "license": "proprietary",
    "support": {
        "email": "support@advancedhosters.com",
        "source": "https://github.com/advancedhosters/hipanel",
        "issues": "https://github.com/advancedhosters/hipanel/issues",
        "wiki": "https://github.com/advancedhosters/hipanel/wiki"
    },
    "authors": [
        {
            "name": "Andrii Vasyliev",
            "email": "sol@hiqdev.com",
            "homepage": "https://hipanel.com/"
        }
    ],
    "require": {
        "yiisoft/yii2": "^2.0",
        "advancedhosters/yii2-asset-advancedhosters": "*@dev",
        "hiqdev/hipanel-core": "*@dev",
        "hiqdev/hipanel-module-dashboard": "*@dev",
        "hiqdev/hipanel-module-client": "*@dev",
        "hiqdev/hipanel-module-ticket": "*@dev",
        "hiqdev/hipanel-module-server": "*@dev",
        "hiqdev/hipanel-module-finance": "*@dev",
        "hiqdev/hipanel-module-hosting": "*@dev",
        "hiqdev/hipanel-module-stock": "*@dev",
        "hiqdev/hisite-module-news": "*@dev",
        "hiqdev/php-merchant": "*@dev",
        "hiqdev/yii2-theme-adminlte": "*@dev",
        "hiqdev/yii2-x-editable": "*@dev",
        "hiqdev/yii2-bootstrap-switch": "*@dev",
        "hiqdev/yii2-asset-hipanel": "*@dev",
        "hiqdev/yii2-hiam-authclient": "*@dev",
        "hiqdev/yii2-hiart": "*@dev",
        "hiqdev/yii2-cart": "*@dev",
        "hiqdev/yii2-combo": "*@dev",
        "hiqdev/yii2-higrid": "*@dev",
        "hiqdev/yii2-merchant": "*@dev",
        "hiqdev/yii2-collection": "*@dev",
        "hiqdev/yii2-menumanager": "*@dev",
        "hiqdev/yii2-thememanager": "*@dev",
        "michelf/php-smartypants": "1.6.*@dev"
    },
    "require-dev": {
        "hiqdev/hidev-php": "<2.0",
        "advancedhosters/hidev-advancedhosters": "<2.0"
    },
    "autoload": {
        "psr-4": {
            "adavancedhosters\\hipanel\\": "src",
            "advancedhosters\\hipanel\\": "src"
        }
    },
    "extra": {
        "config-plugin": {
            "hisite": "src/config/hisite.php"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
